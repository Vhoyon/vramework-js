root = true

[*]
end_of_line = lf
insert_final_newline = true
charset = utf-8
trim_trailing_whitespace = true

[*.yml]
trim_trailing_whitespace = false

[*.ts]
indent_style = tab
indent_size = 4

[Makefile]
indent_style = tab

[lib/**.js]
indent_style = tab

[*.json]
indent_style = space
indent_size = 2
